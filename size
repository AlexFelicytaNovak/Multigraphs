if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 110
normal! 028|
lcd ~/Projects/MultiDiGraph
wincmd w
argglobal
if bufexists(fnamemodify("~/Projects/MultiDiGraph/MultiDiGraph.py", ":p")) | buffer ~/Projects/MultiDiGraph/MultiDiGraph.py | else | edit ~/Projects/MultiDiGraph/MultiDiGraph.py | endif
if &buftype ==# 'terminal'
  silent file ~/Projects/MultiDiGraph/MultiDiGraph.py
endif
balt ~/Projects/MultiDiGraph/tests.py
setlocal fdm=expr
